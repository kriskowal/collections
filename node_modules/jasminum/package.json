{
  "name": "jasminum",
  "version": "2.0.7",
  "description": "A light, modular, promissory, isomorphic Jasmine test scaffold clone",
  "homepage": "http://collectionsjs.com",
  "license": "BSD 3-Clause",
  "keywords": [
    "jasmine",
    "test",
    "testing",
    "bdd",
    "promise",
    "isomorphic"
  ],
  "main": "jasminum.js",
  "browser": {
    "./platform-reporter.js": "./browser/reporter.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kriskowal/collections"
  },
  "bugs": {
    "url": "https://github.com/kriskowal/collections/issues"
  },
  "dependencies": {
    "colors": "^0.6.2",
    "glob": "^3.2.8",
    "@collections/compare": "^1.0.0",
    "@collections/equals": "^1.0.0",
    "@collections/has": "^1.0.0"
  },
  "publishConfig": {
    "tag": "future"
  },
  "bin": {
